switchport mode access
switchport port-security
switchport port-security mac-address sticky
switchport port-security violation restrict
witchport port-security maximum 2
exit
int fa 0/1
spanning-tree bpduguard enable
switchport mode access
exit
vtp mode server
vtp domain gim
vtp password cisco
vlan 200
name GIM_RG
vlan 201
name GIM_VEZ
vlan 202
name GIM_TEL
vlan 203
name GIM_TANAR
vlan 204
name GIM_PORTA
vlan 205
name GIM_GO
vlan 220
name GIM_SERVER
exit
int vlan 200
no sh
ip address 10.2.0.192 255.255.255.0
exit
spanning-tree vlan 201 root primary 
spanning-tree vlan 202 root primary 
spanning-tree vlan 203 root primary 
spanning-tree vlan 204 root primary 
spanning-tree vlan 205 root primary 
spanning-tree vlan 220 root primary 
int g0/1
switchport access trunk
exit
