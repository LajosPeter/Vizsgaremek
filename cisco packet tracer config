# rtr-telekom-core-01
en 
conf t
hostname rtr-telekom-core-01
interface GigabitEthernet0/0
ip address 195.228.6.1 255.255.255.252
no shutdown
exit
interface GigabitEthernet2/0
ip address 195.228.6.5 255.255.255.252
no shutdown
exit





